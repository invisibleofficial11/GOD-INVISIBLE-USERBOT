

_get_repolink () {
    local regex
    regex='(https?)://github.com/.+/.+'
    if [[ $invisibleofficial11_REPO == "GOD-INVISIBLE-USERBOT" ]]
    then
        echo "aHR0cHM6Ly9naXRodWIuY29tL1BST0JPWS1PUC9QUk8tVVNFUkJPVC9hcmNoaXZlL1Byby56aXAlghdiJ3JDTKjh=" | base64 -d
    elif [[ $invisibleofficial11_REPO == "GOD-INVISIBLE-USERBOT" ]]
    then
        echo "aHR0cHM6Ly9naXRodWIuY29tL1BST0JPWS1PUC9QUk8tVVNFUkJPVC9hcmNoaXZlL1Byby56aXAlghdiJ3JDTKjh=" | base64 -d
    elif [[ $invisibleofficial11_REPO =~ $regex ]]
    then
        if [[ $invisibleofficial11_REPO_BRANCH ]]
        then
            echo "${invisibleofficial11_REPO}/archive/${main_REPO_BRANCH}.zip"
        else
            echo "${invisibleofficial11_REPO}/archive/main.zip"
        fi
    else
        echo "aHR0cHM6Ly9naXRodWIuY29tL1BST0JPWS1PUC9QUk8tVVNFUkJPVC9hcmNoaXZlL1Byby56aXAlghdiJ3JDTKjh=" | base64 -d
    fi
}


_pro_bot () {
    local zippath
    zippath="main.zip"
    echo "  ‚ô¶Ô∏è Downloading Pro Invisible-LegendBot Source Code... ‚ô¶Ô∏è"
    wget -q $(_get_repolink) -O "$zippath"
    echo "  üìçUnpacking Data...üìç"
    invisibleofficial11PATH=$(zipinfo -1 "$zippath" | grep -v "/.");
    unzip -qq "$zippath"
    echo " üí´ Download Complete Now Loding UserBot üí´"
    echo "  ‚ú® Loding Powers InvisibleBoy & Legend-X-Pro ‚ú®"
    rm -rf "$zippath"
    sleep 5
    cd $invisibleofficial11PATH
    echo "    üîûLoding Invisible-LegendBotüîû     "
    echo " ‚ô•Ô∏è RUK JA BHAI HO GYA AAB TU GAND MARDE TG KI JA  ‚ô•Ô∏è"

    python3 ../setup/updater.py ../requirements.txt requirements.txt
    python3 -m userbot
}

_pro_bot
