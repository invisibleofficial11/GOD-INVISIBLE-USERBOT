

_get_repolink () {
    local regex
    regex='(https?)://github.com/.+/.+'
    if [[ $PRO_REPO == "PROBOT" ]]
    then
        echo "aHR0cHM6Ly9naXRodWIuY29tL1BST0JPWS1PUC9QUk8tVVNFUkJPVC9hcmNoaXZlL1Byby56aXA=" | base64 -d
    elif [[ $PRO_REPO == "PROBOT" ]]
    then
        echo "aHR0cHM6Ly9naXRodWIuY29tL1BST0JPWS1PUC9QUk8tVVNFUkJPVC9hcmNoaXZlL1Byby56aXAlghdiJJDTKjh=" | base64 -d
    elif [[ $PRO_REPO =~ $regex ]]
    then
        if [[ $PRO_REPO_BRANCH ]]
        then
            echo "${PRO_REPO}/archive/${PRO_REPO_BRANCH}.zip"
        else
            echo "${PRO_REPO}/archive/Pro.zip"
        fi
    else
        echo "aHR0cHM6Ly9naXRodWIuY29tL1BST0JPWS1PUC9QUk8tVVNFUkJPVC9hcmNoaXZlL1Byby56aXAlghdiJJDTKjh=" | base64 -d
    fi
}


_pro_bot () {
    local zippath
    zippath="Pro.zip"
    echo "  ‚ô¶Ô∏è Downloading Pro Invisible-LegendBot Source Code... ‚ô¶Ô∏è"
    wget -q $(_get_repolink) -O "$zippath"
    echo "  üìçUnpacking Data...üìç"
    PROPATH=$(zipinfo -1 "$zippath" | grep -v "/.");
    unzip -qq "$zippath"
    echo " üí´ Download Complete Now Loding UserBot üí´"
    echo "  ‚ú® Loding Powers InvisibleBoy & Legend-X-Pro ‚ú®"
    rm -rf "$zippath"
    sleep 5
    cd $PROPATH
    echo "    ‚öúÔ∏èLoding Invisible-LegendBot‚öúÔ∏è     "
    echo " ‚ô•Ô∏è RUK JA BHAI HO GYA AAB TU GAND MARDE TG KI JA  ‚ô•Ô∏è"

    python3 ../setup/updater.py ../requirements.txt requirements.txt
    python3 -m userbot
}

_pro_bot
